    agent any

    stages {
        stage('Insall dependecies') {
            steps {
                echo 'I am installing dependencies'
            }
        }
        stage('Buiild') {
            steps {
                echo 'I am building the artifact'
            }
        }
        stage('Test') {
            steps {
                echo 'I am testing the application'
            }
        }
        stage('Deploy') {
            steps {
                echo 'I am deploying the application'
            }
        }
    }
}
